  <% require 'pry' %>
  <!-- # create a new line chart -->
  <% chart = PiCharts::Line.new %>
  
  <head>  
  <script src="//www.google.com/jsapi"></script>
 
   <script src="/javascripts/chartkick.js"></script>
   
   <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.27.0/moment.min.js" integrity="sha512-rmZcZsyhe0/MAjquhTgiUcb4d9knaFc7b5xAfju483gbEXTkeJRUMIPk6s3ySZMYUHEcjKbjLjyddGWMrNEvZg==" crossorigin="anonymous"></script>
 
  <link rel="stylesheet" href="/stylesheets/statistics/statistics.css">
   <% if !@entries.empty? %>
      <%= chart.cdn %>
   <%end %>

  </head>

  <body>
    <div id="ctn-phone-top" class="container">
    <br>
     

    <h1 class="medications"> <%=@user.firstname%>'s Statistics</h1>

    <div class="graphs">
      
      <br>

      <% if !@entries.empty? %>
        <% @data.each do |k, v|%>
          <div class="chart-statistics">
            <div class= "container-mood-energy">
              <div class="mood-left">
                <p class="mood-level-p">Mood Levels 1 - 5</p>
                <img class='moods' src='/stylesheets/images/mood/Moods.png' alt='ZenRx Moods'/>
              </div>
              <div class="energy-right">
                <p class="energy-level-p">Energy Levels 1 - 5</p>
                <img class='batteries' src='/stylesheets/images/energy/Energies.png' alt='ZenRx Energies'/>
              </div>
            </div>
              <%= line_chart entry_graph_data(v) %>
          </div>
        <% end %>
      <% else %>
        <h1 class="chart-not-enough"> Not enough entries to construct chart!<h1>
      <% end %>

        <div class="cobra"></div> 
        <div class="btn-bottom into-footer">
          <button type="button" class="btn btn-secondary left" data-toggle="popover" title="Graphs & Charts" data-content="Coming Soon! Still under construction!"><img  src="/charts/chart.png" alt="ZenRx Chart"/><text class="button-graph"> Graphs & Charts</text></button>
          <button type="button" class="btn btn-secondary right" data-toggle="popover" title="Printable Reports" data-content="Coming Soon! Still under construction!"><img  src="/charts/pdf.png" alt="ZenRx Chart"/><text class="button-graph"> Printable Reports</text></button>
        </div>
    </div>

  
    
  </body>

  <div class="push"></div> 
</div>